{
  "name": "marketplace-frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn lint && yarn prettier:check && yarn guidelines:check && next build",
    "start": "next start",
    "lint": "eslint 'pages/**' 'src/App/**' 'src/api/**' 'src/_types/**' 'scripts/**'",
    "lint:fix": "yarn lint --fix",
    "prettier:base": "prettier 'pages/**' 'src/App/**' 'src/api/**' 'src/_types/**' 'scripts/**'",
    "prettier:check": "yarn prettier:base --check",
    "prettier:write": "yarn prettier:base --write",
    "prettier:other": "prettier --write '**/*.(md|json)'",
    "tsc:check": "tsc --noEmit",
    "guidelines:check": "ts-node scripts/checkGuidelines.ts",
    "guidelines:write": "ts-node scripts/writeGuidelines.ts",
    "delete:emptyDirs": "find ./src -type d -empty -delete",
    "hooks:install": "husky install",
    "hooks:uninstall": "git config --unset core.hooksPath"
  },
  "lint-staged": {
    "**/*.(ts|tsx)": [
      "eslint --fix",
      "prettier --write",
      "yarn guidelines:check"
    ],
    "**/*.(md|json)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@bonfida/spl-name-service": "0.1.22",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@metamask/jazzicon": "^2.0.0",
    "@metaplex-foundation/mpl-token-metadata": "^0.0.2",
    "@metaplex/js": "4.1.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.4.1",
    "@mui/x-data-grid": "^5.5.1",
    "@react-hook/debounce": "^4.0.0",
    "@solana/spl-token-registry": "^0.2.2268",
    "@solana/wallet-adapter-base": "^0.9.2",
    "@solana/wallet-adapter-react": "^0.15.2",
    "@solana/wallet-adapter-react-ui": "^0.9.4",
    "@solana/wallet-adapter-wallets": "^0.14.2",
    "@solana/web3.js": "^1.31.0",
    "autosuggest-highlight": "^3.2.1",
    "axios": "^0.25.0",
    "axios-auth-refresh": "^3.2.2",
    "bn.js": "^5.2.0",
    "bs58": "^4.0.1",
    "clsx": "^1.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "elastic-builder": "^2.16.0",
    "html-react-parser": "^1.4.10",
    "intercept-stdout": "^0.1.2",
    "luxon": "^2.3.0",
    "nanoid": "^3.2.0",
    "next": "12.0.10",
    "qs": "^6.10.3",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-flow-renderer": "^9.7.3",
    "react-intersection-observer": "^8.33.1",
    "react-json-view": "^1.21.3",
    "react-use": "^17.3.2",
    "react-virtualized": "^9.22.3",
    "recoil": "^0.6.1",
    "recoil-persist": "^4.0.0",
    "sharp": "^0.30.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.3",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/luxon": "^2.0.9",
    "@types/node": "17.0.8",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.38",
    "@types/react-virtualized": "^9.21.16",
    "@types/sharp": "^0.30.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "ansi-colors": "^4.1.1",
    "async-retry": "^1.3.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fast-glob": "^3.2.11",
    "fastq": "^1.13.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.4"
  },
  "version": "0.0.0"
}
